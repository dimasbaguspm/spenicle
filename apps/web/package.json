{
  "name": "bun-react-template",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun run generate:css && bun --hot cmd/dev.ts",
    "build": "bun run generate:css && bun build index.html --outdir=./dist --minify",
    "generate:css": "npx @tailwindcss/cli -i src/index.css -o public/index.css --minify",
    "generate:openapi-types": "bun run scripts/generate-openapi-types.ts"
  },
  "dependencies": {
    "@dimasbaguspm/versaur": "^0.0.63",
    "@tanstack/react-query": "^5.90.16",
    "axios": "^1.13.2",
    "bun-plugin-tailwind": "^0.1.2",
    "class-variance-authority": "^0.7.1",
    "dayjs": "^1.11.19",
    "lodash": "^4.17.21",
    "lucide-react": "^0.562.0",
    "query-string": "^9.3.1",
    "react": "^19",
    "react-dom": "^19",
    "react-hook-form": "^7.69.0",
    "react-router": "^7.11.0",
    "react-swipeable": "^7.0.2",
    "recharts": "^3.6.0",
    "tailwindcss": "^4.1.11"
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.1.18",
    "@types/bun": "latest",
    "@types/lodash": "^4.17.21",
    "@types/react": "^19",
    "@types/react-dom": "^19"
  }
}
